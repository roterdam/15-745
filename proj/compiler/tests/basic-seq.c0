int A_constr(int i){
  return i*2;
}

int B_constr(int i){
  return i+2;
}

int add(int a, int b){
    return a+b;
}

int main() {
  int<> A = tabulate A_constr 10000;
  int<> B = tabulate B_constr 10000;

  int<> C = combine add A B;

  int<> D = seq(1..10000);
  
  return add(reduce add 0 C, reduce add 0 D);
}
